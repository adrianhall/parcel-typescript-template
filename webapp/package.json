{
  "name": "parcel-typescript-template-webapp",
  "version": "1.0.0",
  "description": "A template for a react app",
  "author": "Adrian Hall <photoadrian@outlook.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "run-s clean lint test",
    "build": "parcel build src/index.html --no-source-maps",
    "clean": "rimraf ./dist",
    "predeploy": "run-s build",
    "deploy": "node ./scripts/deploy_to_azure.js",
    "lint": "run-s lint:code lint:style",
    "lint:code": "eslint --ext ts,tsx src",
    "lint:style": "stylelint src/**/*.scss",
    "start": "parcel src/index.html --open",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collect-coverage"
  },
  "dependencies": {
    "antd": "^4.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@azure/storage-blob": "^12.1.1",
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-ant-design": "0.0.1",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.17",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-standard": "^4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.4",
    "mime-types": "^2.1.26",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "readdirp": "^3.4.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-strict-scss": "^0.5.0",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  }
}
